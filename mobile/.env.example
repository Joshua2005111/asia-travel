// Environment configuration for Expo
// Copy this file to .env and fill in your values

// API Configuration
EXPO_PUBLIC_API_URL=http://localhost:3000/api

// Stripe Configuration
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_key

// Amap (Gaode) Configuration
EXPO_PUBLIC_AMAP_KEY=your_amap_key

// Google Maps (iOS)
EXPO_PUBLIC_GOOGLE_MAPS_IOS_KEY=your_google_maps_ios_key

// OpenAI Configuration
EXPO_PUBLIC_OPENAI_MODEL=gpt-4

// Feature Flags
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=false
EXPO_PUBLIC_ENABLE_ANALYTICS=true
